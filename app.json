{
  "expo": {
    "name": "dashboard",
    "slug": "dashboard",
    "scheme": "dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marobacoffee.dashboard"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://evanbacon.dev/"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://evanbacon.dev/"
      },
      "eas": {
        "projectId": "2bd91d97-930a-4c34-b8cb-69526c14295a"
      }
    }
  }
}
